{
  "name": "cypress-react-unit-test",
  "version": "0.0.0-development",
  "description": "Unit test React components using Cypress",
  "main": "dist",
  "no-module-for-now": "lib",
  "types": "lib",
  "scripts": {
    "test": "cypress run",
    "percy:test": "percy exec -- cypress run",
    "build": "npm run transpile",
    "cy:open": "cypress open",
    "transpile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run lint && npm run transpile",
    "lint": "standard --verbose --fix *.js src cypress/integration",
    "semantic-release": "semantic-release",
    "format": "prettier --write '*.js', 'src/*.jsx' 'cypress/**/*.js' 'cypress/**/*.jsx' 'lib/*.ts'",
    "check:links": "find . -type f -name 'README.md' ! -path './node_modules/*' | xargs -L1 npx markdown-link-check --quiet",
    "stop-only": "stop-only --folder cypress",
    "warn-only": "stop-only --warn --folder cypress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run warn-only",
      "pre-push": "npm run stop-only"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist",
    "lib",
    "support",
    "plugins"
  ],
  "keywords": [
    "react",
    "cypress",
    "cypress-io",
    "test",
    "testing"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@applitools/eyes-cypress": "3.13.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.10.4",
    "@bahmutov/cy-api": "1.4.2",
    "@date-io/date-fns": "1",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@emotion/core": "10.0.22",
    "@material-ui/core": "4.9.5",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.39",
    "@material-ui/pickers": "3.2.10",
    "@percy/cypress": "2.3.1",
    "@testing-library/cypress": "5.3.1",
    "@types/node": "9.6.49",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "arg": "4.1.3",
    "autoprefixer": "9.7.6",
    "axios": "0.18.1",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "4.0.0",
    "bootstrap": "4.4.1",
    "check-code-coverage": "1.9.2",
    "common-tags": "1.8.0",
    "css-loader": "2.1.1",
    "cypress": "5.2.0",
    "cypress-axe": "0.8.1",
    "cypress-image-snapshot": "3.1.1",
    "cypress-plugin-snapshots": "1.4.4",
    "cypress-react-selector": "2.0.2",
    "date-fns": "2.13.0",
    "happo-cypress": "1.9.1",
    "happo.io": "5.6.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "lodash": "4.17.15",
    "markdown-link-check": "3.8.1",
    "node-sass": "4.14.1",
    "prettier": "1.19.1",
    "pretty": "2.0.0",
    "prop-types": "15.7.2",
    "radioactive-state": "1.3.4",
    "react": "^16.13.1",
    "react-bootstrap": "1.0.1",
    "react-calendar-heatmap": "1.8.1",
    "react-dom": "^16.13.1",
    "react-google-maps": "9.4.5",
    "react-loading-skeleton": "2.0.1",
    "react-router": "6.0.0-alpha.1",
    "react-router-dom": "6.0.0-alpha.1",
    "react-scripts": "3.4.1",
    "react-tooltip": "4.2.6",
    "semantic-release": "17.2.3",
    "standard": "14.3.3",
    "stop-only": "3.1.0",
    "style-loader": "0.23.1",
    "styled-components": "5.0.0",
    "svg-url-loader": "3.0.3",
    "tailwindcss": "1.4.6",
    "typescript": "3.9.7",
    "victory": "34.3.6",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.9"
  },
  "standard": {
    "globals": [
      "Cypress",
      "cy",
      "expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/cypress-react-unit-test.git"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.10.4",
    "@cypress/code-coverage": "3.8.1",
    "@cypress/webpack-preprocessor": "5.4.5",
    "@mdx-js/loader": "^1.6.16",
    "@next/mdx": "^9.5.3",
    "@zeit/next-sass": "^1.0.1",
    "babel-plugin-istanbul": "6.0.0",
    "debug": "4.1.1",
    "find-webpack": "2.0.0",
    "framer-motion": "2.6.13",
    "i18next": "19.7.0",
    "mime-types": "2.1.26",
    "next": "^9.5.3",
    "react-i18next": "11.7.2",
    "unfetch": "4.1.0"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "feature/cypress-mount-mode",
        "prerelease": "cypress-mount-mode"
      }
    ]
  }
}
